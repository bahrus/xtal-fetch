{
  "name": "xtal-fetch",
  "version": "0.0.89",
  "keywords": [
    "fetch",
    "rest",
    "web-components",
    "custom-elements"
  ],
  "description": "Dependency free web component wrapper around the fetch function.",
  "main": "xtal-fetch-get.js",
  "module": "xtal-fetch-get.js",
  "repository": "https://github.com/bahrus/xtal-fetch",
  "author": "bahrus <anderson.bruce.b@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "trans-render": "0.0.290",
    "xtal-element": "0.0.402"
  },
  "devDependencies": {
    "aggregator-fn": "0.0.27",
    "for-instance": "0.0.27",
    "if-diff": "0.0.70",
    "json-file-patch": "0.0.1",
    "litter-g": "0.0.36",
    "on-to-me": "0.0.23",
    "xtal-test": "0.0.51",
    "@skypack/package-check": "0.2.2",
    "@custom-elements-manifest/analyzer": "0.4.10"
  },
  "files": [
    "xtal-*.*",
    "custom-elements.json",
    "types.d.ts"
  ],
  "exports": {
    ".": "./xtal-fetch.js",
    "./xtal-fetch-get.js": "./xtal-fetch-get.js",
    "./xtal-fetch-req.js": "./xtal-fetch-req.js",
    "./xtal-fetch-entities.js": "./xtal-fetch-entities.js",
    "./xtal-fetch.js": "./xtal-fetch.js"
  },
  "typings": "types.d.ts",
  "scripts": {
    "serve": "node node_modules/xtal-test/serve.js",
    "test": "node test/test-all.js",
    "package-check": "package-check",
    "doc": "custom-elements-manifest analyze"
  },
  "customElements": "custom-elements.json"
}
